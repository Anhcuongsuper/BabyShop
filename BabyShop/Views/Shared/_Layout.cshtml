</head>
<body>
    <section>
        <div class="container">
            <div class="header" id="home1">
                <div class="container">
                    @{
                        var session = (UserLogin)Session[BabyShop.Common.CommonConstants.USER_SESSION];
                    }
                    <div class="w3l_login" style="display:flex; padding:5px">
                        @if (session == null)
                        {
                            <a href="/dang-nhap" data-toggle="modal" style="margin-right:15px">Đăng nhập</a>
                            <a href="/dang-ky" data-toggle="modal">Đăng ký</a>
                        }
                        else
                        {
                            <p>Xin chào :</p><span>@session.UserName</span>
                            <a href="/User/Logout" data-toggle="modal" style="margin-right:15px">Đăng xuất</a>

                        }
                    </div>
                </div>
            </div>
        </div>
    </section>
    <section id="header" class="clearfix">
        <div class="container">
            <div class="row">
                <div class="col-sm-12 header">
                    <div class="col-sm-12 header_left">
                        <nav class="navbar navbar-default navbar-fixed-top">
                            @{Html.RenderAction("MainMenu", "Home");}
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </section>


    @RenderSection("slide", required: false)




    <!-- //banner -->
    <!-- banner-bottom -->
    <!-- //modal-video -->
    @RenderBody();
    <!-- //newsletter -->
    <!-- footer -->
    <div class="footer">
        @{Html.RenderAction("Footer", "Home");}
    </div>
    <!-- //footer -->

    <script src="/Content/client/js/classie.js"></script>
    <script src="/Content/client/js/cbpAnimatedHeader.js"></script>
</body>
